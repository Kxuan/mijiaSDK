<?xml version="1.0" encoding="UTF-8"?>
<root xmlns="urn:schemas-mi-com:device-1-0">
    <specVersion>
        <major>1</major>
        <minor>0</minor>
    </specVersion>
    <device>
        <type>urn:schemas-mi-com:device:Zhimi:AirPurifier:6</type>
        <friendlyName>米家空气净化器Pro</friendlyName>
        <firmwareVersion>6</firmwareVersion>
        <modelName>zhimi.airpurifier.v6</modelName>
        <modelNumber>2016</modelNumber>
        <modelDescription>米家空气净化器Pro</modelDescription>
        <manufacturer>Xiaomi</manufacturer>
        <manufacturerURL>http://www.mi.com</manufacturerURL>
        
        <serviceList>
            <service>
                <type>urn:schemas-mi-com:service:Airpurifier:MService:1</type>
                <description>空气净化服务</description>

                <propertyList>
                    <property gettable="true" notifiable="true">
                        <friendlyName>AQI</friendlyName>
                        <internalName>aqi</internalName>
                        <description>空气质量指数</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>600</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>Bright</friendlyName>
                        <internalName>bright</internalName>
                        <description>光亮传感器当前值</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>200</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>ChildLock</friendlyName>
                        <internalName>child_lock</internalName>
                        <description>童锁开关</description>
                        <dataType>string</dataType>
                        <allowedValueList>
                            <allowedValue>on</allowedValue>
                            <allowedValue>off</allowedValue>
                        </allowedValueList>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>Filter1Life</friendlyName>
                        <internalName>filter1_life</internalName>
                        <description>滤芯寿命所剩百分比</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>100</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>Mode</friendlyName>
                        <internalName>mode</internalName>
                        <description>工作档位</description>
                        <dataType>string</dataType>
                        <allowedValueList>
                            <allowedValue>idle</allowedValue>
                            <allowedValue>auto</allowedValue>
                            <allowedValue>silent</allowedValue>
                            <allowedValue>strong</allowedValue>
                            <allowedValue>high</allowedValue>
                            <allowedValue>medium</allowedValue>
                            <allowedValue>low</allowedValue>
                            <allowedValue>favorite</allowedValue>
                        </allowedValueList>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>MornMode</friendlyName>
                        <internalName>morn_mode</internalName>
                        <description>自动开机学习模式</description>
                        <dataType>string</dataType>
                        <allowedValueList>
                            <allowedValue>poweroff</allowedValue>
                        </allowedValueList>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>MornDataNum</friendlyName>
                        <internalName>morn_data_num</internalName>
                        <description>累计学习到的用户数据个数</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>30</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>MornTime</friendlyName>
                        <internalName>morn_time</internalName>
                        <description>计算出来的早晨自动关机时间</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>86399</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>SleepMode</friendlyName>
                        <internalName>sleep_mode</internalName>
                        <description>睡眠学习模式</description>
                        <dataType>string</dataType>
                        <allowedValueList>
                            <allowedValue>poweroff</allowedValue>
                            <allowedValue>silent</allowedValue>
                        </allowedValueList>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>SleepTime</friendlyName>
                        <internalName>sleep_time</internalName>
                        <description>计算出来的当天自动睡眠时间</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>86399</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>PurifyVolume</friendlyName>
                        <internalName>purify_volume</internalName>
                        <description>设备累计净化空气量</description>
                        <dataType>long</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>99999999</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>UseTime</friendlyName>
                        <internalName>use_time</internalName>
                        <description>设备运行时间</description>
                        <dataType>long</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>2147483647</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>VolumeValue</friendlyName>
                        <internalName>volume</internalName>
                        <description>音量调节</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>100</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>TempDec</friendlyName>
                        <internalName>temp_dec</internalName>
                        <description>温度x10</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>-400</minimum>
                            <maximum>1250</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>LedState</friendlyName>
                        <internalName>led</internalName>
                        <description>led开关状态</description>
                        <dataType>string</dataType>
                        <allowedValueList>
                            <allowedValue>on</allowedValue>
                            <allowedValue>off</allowedValue>
                        </allowedValueList>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>ActMornMode</friendlyName>
                        <internalName>act_morn</internalName>
                        <description>早上自动关机功能 运行模式</description>
                        <dataType>string</dataType>
                        <allowedValueList>
                            <allowedValue>single</allowedValue>
                            <allowedValue>close</allowedValue>
                        </allowedValueList>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>ActSleepMode</friendlyName>
                        <internalName>act_sleep</internalName>
                        <description>自动睡眠功能 运行模式</description>
                        <dataType>string</dataType>
                        <allowedValueList>
                            <allowedValue>single</allowedValue>
                            <allowedValue>close</allowedValue>
                        </allowedValueList>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>F1TotalHour</friendlyName>
                        <internalName>f1_hour</internalName>
                        <description>滤芯总寿命</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>2000</minimum>
                            <maximum>6000</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>F1HourUsed</friendlyName>
                        <internalName>f1_hour_used</internalName>
                        <description>:滤芯使用时长</description>
                        <dataType>int</dataType>
                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>6000</maximum>
                        </allowedValueRange>
                    </property>
                    <property gettable="true" notifiable="true">
                        <friendlyName>FavoriteLevel</friendlyName>
                        <internalName>favorite_level</internalName>
                        <description>最爱档对应档值</description>
                        <dataType>int</dataType>
<!--                        <allowedValueRange>
                            <minimum>0</minimum>
                            <maximum>16</maximum>
                        </allowedValueRange>-->
                    </property>
                </propertyList>
                
                <actionList>
                    <action>
                        <friendlyName>setMode</friendlyName>
                        <internalName>set_mode</internalName>
                        <description>设置模式</description>
                        <argumentList>
                            <argument>
                                <name>Mode</name>
                                <direction>in</direction>
                                <relatedProperty>Mode</relatedProperty>
                            </argument>
                        </argumentList>
                    </action>
                    <action>
                        <friendlyName>SetLedState</friendlyName>
                        <internalName>set_led</internalName>
                        <description>设置led开关</description>
                        <argumentList>
                            <argument>
                                <name>LedState</name>
                                <direction>in</direction>
                                <relatedProperty>LedState</relatedProperty>
                            </argument>
                        </argumentList>
                    </action>
                    <action>
                        <friendlyName>setVolumeValue</friendlyName>
                        <internalName>set_volume</internalName>
                        <description></description>
                        <argumentList>
                            <argument>
                                <name>VolumeValue</name>
                                <direction>in</direction>
                                <relatedProperty>VolumeValue</relatedProperty>
                            </argument>
                        </argumentList>
                    </action>
                    <action>
                        <friendlyName>resetActMorn</friendlyName>
                        <internalName>reset_act_morn</internalName>
                        <description>重置自动开机时间</description>
                        <argumentList />
                    </action>
                    <action>
                        <friendlyName>resetActSleep</friendlyName>
                        <internalName>reset_act_sleep</internalName>
                        <description>重置自动睡眠时间</description>
                        <argumentList />
                    </action>
                    <action>
                        <friendlyName>resetFilter1</friendlyName>
                        <internalName>reset_filter1</internalName>
                        <description>重置滤芯</description>
                        <argumentList></argumentList>
                    </action>
                    <action>
                        <friendlyName>setActMornMode</friendlyName>
                        <internalName>set_act_morn</internalName>
                        <description>设置自动关机模式</description>
                        <argumentList>
                            <argument>
                                <name>ActMornMode</name>
                                <direction>in</direction>
                                <relatedProperty>ActMornMode</relatedProperty>
                            </argument>
                        </argumentList>
                    </action>
                    <action>
                        <friendlyName>setActSleepMode</friendlyName>
                        <internalName>set_act_sleep</internalName>
                        <description>设置自动睡眠模式</description>
                        <argumentList>
                            <argument>
                                <name>ActSleepMode</name>
                                <direction>in</direction>
                                <relatedProperty>ActSleepMode</relatedProperty>
                            </argument>
                        </argumentList>
                    </action>
                    <action>
                        <friendlyName>setChildLock</friendlyName>
                        <internalName>set_child_lock</internalName>
                        <description>设置童锁</description>
                        <argumentList>
                            <argument>
                                <name>ChildLock</name>
                                <direction>in</direction>
                                <relatedProperty>ChildLock</relatedProperty>
                            </argument>
                        </argumentList>
                    </action>
                    <action>
                        <friendlyName>setF1TotalHour</friendlyName>
                        <internalName>set_f1_hour</internalName>
                        <description>设置滤芯总寿命</description>
                        <argumentList>
                            <argument>
                                <name>F1TotalHour</name>
                                <direction>in</direction>
                                <relatedProperty>F1TotalHour</relatedProperty>
                            </argument>
                        </argumentList>
                    </action>
                    <action>
                        <friendlyName>setF1HourUsed</friendlyName>
                        <internalName>set_f1_hour_used </internalName>
                        <description>设置滤芯使用时长</description>
                        <argumentList>
                            <argument>
                                <name>F1HourUsed</name>
                                <direction>in</direction>
                                <relatedProperty>F1HourUsed</relatedProperty>
                            </argument>
                        </argumentList>
                    </action>
                    <action>
                        <friendlyName>setLevelFavorite</friendlyName>
                        <internalName>set_level_favorite</internalName>
                        <description>设置喜爱模式等级</description>
                        <argumentList>
                            <argument>
                                <name>FavoriteLevel</name>
                                <direction>in</direction>
                                <relatedProperty>FavoriteLevel</relatedProperty>
                            </argument>
                        </argumentList>
                    </action>
                </actionList>
            </service>
        </serviceList>
    </device>
</root>